{
    "author": "SeungJe Jun",
    "manifest_version": 2,
    "name": "RememberMe",
    "description": "Store login info and show that based on url that you have been",
    "version": "0.0.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html",
        "default_title": "RememberMe"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "webRequest",
        "<all_urls>",
        "http://localhost:3000/"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}